{
    "fov": 90,
    "camera_height": 25.5,
    "camera_angle": -5,
    "output_size": [640, 480],
    "rescale": 8,
    "chunk_size_cm": 250,
    "chunk_size_px": 1000,
    "shuffle": true,
    "seed": 123456,
    "start_line_rows": 3,
    "start_line_colums":20,
    "patch_size": 20,

    "obstacle_class": 0,

    "paths": {
        "annotations_output_path": "data/annotations/{splitname}",
        "images_output_path": "data/images/{splitname}",
        "manual_annotations_input_path": "real_dataset/annotations/set_*",
        "manual_images_input_path": "real_dataset/images/set_*",
        "chunk_path": "chunks",
        "chunk_file_pattern": "{chunk_type}_{variant}",
        "output_file_pattern": "image_{idx}.jpg"
    },

    "splits": {
        "train_split": {
            "size": 50,
            "fraction_synthetic": 0.5,
            "fraction_augmented": 0.5
        },
        "validation_split": {
            "size": 50,
            "fraction_synthetic": 0.5,
            "fraction_augmented": 0.5
        },
        "test_split": {
            "size": 50,
            "fraction_synthetic": 0.5,
            "fraction_augmented": 0.5
        }
    },
    "disturbances": [
        { "HiddenGroundRect": [] },
        { "BoxObstacle": [] },
        { "BoxObstacle": [] },
        { "Dust": [100000, 63] },
        { "Dust": [100000, 127] },
        { "Dust": [10000, 255] },
        { "RandomBrightness": [] },
        { "RandomClipMax": [] },
        { "RandomClipMax": [] },
        { "DrunkDriving": [] }
    ]
}
